<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="myapp.com.etc.lenovo.bascketball.Entity" >
	<class name="user" table="user" >
		<id name="user_id" column="user_id" type="java.lang.Integer" >
			<generator class="native"/>
		</id>
		<property name="user_name" column="user_name" type="java.lang.String" />
		<property name="password" column="password" type="java.lang.String" />
		<property name="avatar" column="avatar" type="java.lang.String" />
		<property name="sex" column="sex" type="java.lang.String" />
		<property name="phone" column="phone" type="java.lang.String" />
		<property name="birthday" column="birthday" type="java.lang.String" />
	</class>
</hibernate-mapping>